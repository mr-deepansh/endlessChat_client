{
  "name": "endlesschatt",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "vite preview --host --port 4173",
    "start:render": "vite preview --host --port $PORT",
    "lint": "eslint . --max-warnings 10",
    "lint:fix": "eslint . --max-warnings 10 --fix",
    "lint:strict": "eslint . --max-warnings 0",
    "lint:ci": "eslint . --max-warnings 0 --format json --output-file eslint-report.json",
    "lint:report": "eslint . --format html --output-file eslint-report.html",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md,html}\" --config .prettierrc",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md,html}\" --config .prettierrc",
    "format:all": "prettier --write . --config .prettierrc",
    "format:staged": "prettier --write --config .prettierrc",
    "type-check": "tsc --noEmit",
    "quality": "npm run type-check && npm run lint && npm run format:check",
    "quality:ci": "npm run type-check && npm run lint:strict && npm run format:check",
    "fix": "npm run lint:fix && npm run format",
    "precommit": "npm run quality",
    "prepush": "npm run quality:ci",
    "analyze": "vite build --mode production && npx vite-bundle-analyzer dist/stats.html"
  },
